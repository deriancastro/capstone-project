{
  "name": "capstone-project",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:client dev:server",
    "dev:client": "cd client && npm start",
    "dev:server": "nodemon server.js",
    "test": "cd client && npm test",
    "storybook": "cd client && npm run storybook",
    "build": "cd client && npm run build",
    "postinstall": "cd client && npm install",
    "install-client": "cd client && npm install",
    "server": "nodemon server.js",
    "start": "node server.js"
  },
  "engines": {
    "node": "14.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deriancastro/capstone-project.git"
  },
  "author": "Derian Castro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deriancastro/capstone-project/issues"
  },
  "homepage": "https://github.com/deriancastro/capstone-project#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.9",
    "mongoose": "^5.12.14",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5"
  },
  "description": ""
}
